{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {

			"@UI/*": [
				"./src/components/ui/*"
			],
			"@mocks/*": [
				"./src/mocks/*"
			],
			"@views/*": [
				"./src/views/*"
			],
			"@context/*": [
				"./src/context/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@assets/*": [
				"./src/assets/*"
			],
			"@components/*": [
				"./src/components/*"
			]
		}
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}